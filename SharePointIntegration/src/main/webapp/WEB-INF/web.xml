<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ListFiles</servlet-name>
  	<servlet-class>com.qualiantech.sharepoint.Controller.ListFiles</servlet-class>
  </servlet>
  
   <servlet>
  	<servlet-name>ListFilesFromDirectory</servlet-name>
  	<servlet-class>com.qualiantech.sharepoint.Controller.ListFilesFromDirectory</servlet-class>
  </servlet>
   <servlet>
  	<servlet-name>DeleteFiles</servlet-name>
  	<servlet-class>com.qualiantech.sharepoint.Controller.DeleteFiles</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DownloadFile</servlet-name>
  	<servlet-class>com.qualiantech.sharepoint.Controller.DownloadFile</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>ListFiles</servlet-name>
  	<url-pattern>/ListFiles</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ListFilesFromDirectory</servlet-name>
  	<url-pattern>/ListFilesFromDirectory</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
  	<servlet-name>DeleteFiles</servlet-name>
  	<url-pattern>/DeleteFiles</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
  	<servlet-name>DownloadFile</servlet-name>
  	<url-pattern>/DownloadFile</url-pattern>
  </servlet-mapping>
 </web-app>
